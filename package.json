{
  "name": "replacing-deprecated-apis",
  "displayName": "replacing-deprecated-APIs",
  "description": "projeto final da disciplina Top. Avanv. Eng. da Comp.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "replacing-deprecated-apis.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "replacing-deprecated-apis.insertPromptSelection",
        "title": "Corrigir Seleção INSERT PROMPT"
      },
      {
        "command": "replacing-deprecated-apis.replaceApiSelection",
        "title": "Corrigir Seleção REPLACE API"
      },


      {
        "command": "replacing-deprecated-apis.insertPromptFile",
        "title": "Corrigir Arquivo INSERT PROMPT"
      },
      {
        "command": "replacing-deprecated-apis.replaceApiFile",
        "title": "Corrigir Arquivo REPLACE API"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "replacing-deprecated-apis.insertPromptSelection",
          "when": "editorHasSelection",
          "group": "navigation"
        },
        {
          "command": "replacing-deprecated-apis.replaceApiSelection",
          "when": "editorHasSelection",
          "group": "navigation"
        },



        {
          "command": "replacing-deprecated-apis.insertPromptFile",
          "when": "activeEditor",
          "group": "navigation"
        },
        {
          "command": "replacing-deprecated-apis.replaceApiFile",
          "when": "activeEditor",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "openai": "^5.12.0",
    "together-ai": "^0.21.0"
  }
}
